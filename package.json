{
  "name": "node.ts-cli",
  "version": "0.1.0",
  "description": "A Node.js framework for creating Node APIs with Typescript and Express",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "bin": {
    "node.ts": "./bin/index.js"
  },
  "files": [
    "bin/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmwny/node.ts"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "inquirer": "^6.4.1",
    "log-symbols": "^3.0.0",
    "ora": "^3.4.0",
    "shelljs": "^0.8.3",
    "simple-git": "^1.121.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/shelljs": "^0.8.5",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "prepare": "npm run dev",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "test": "jest --config jest.config.js",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "dev": "rimraf bin && npm run build && node static.js"
  },
  "keywords": [
    "Node.js",
    "Typescript",
    "Node.ts",
    "Node",
    "API",
    "Server",
    "CLI"
  ],
  "author": "Matt Mawhinney me@mattmawhinney.com (http://mattmawhinney.com)",
  "license": "MIT"
}
