{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],

  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": [true, "single"],
    "no-var-requires": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "trailing-comma": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
